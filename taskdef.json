{
  "family": "bluegreen-task",
  "executionRoleArn": "arn:aws:iam::580801916917:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "bluegreen-task",
      "image": "<IMAGE_URI>",
      "portMappings": [
        {
          "containerPort": 8080
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512"
}
